{
  "name": "amptech/laravel-blade-scaffold",
  "description": "Generador automático de vistas CRUD Blade para Laravel con componentes reutilizables y detección inteligente de tipos de campo",
  "type": "library",
  "version": "1.0.0",
  "homepage": "https://github.com/oamperezp/laravel-blade-scaffold",
  "license": "MIT",
  "keywords": [
    "laravel",
    "blade",
    "crud",
    "scaffold",
    "generator",
    "jetstream",
    "tailwind",
    "bootstrap",
    "forms",
    "views",
    "artisan",
    "command"
  ],
  "authors": [
    {
      "name": "Oscar Amperez",
      "email": "oamperezp@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2|^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "AmpTech\\LaravelBladeScaffold\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AmpTech\\LaravelBladeScaffold\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AmpTech\\LaravelBladeScaffold\\BladeScaffoldServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "support": {
    "issues": "https://github.com/oamperezp/laravel-blade-scaffold/issues",
    "source": "https://github.com/oamperezp/laravel-blade-scaffold",
    "docs": "https://github.com/oamperezp/laravel-blade-scaffold#readme"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/oamperezp"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
